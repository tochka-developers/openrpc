{
    "name": "tochka-developers/openrpc",
    "description": "OpenRpc extension for tochka-developers/jsonrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Babushkin",
            "email": "darkdarin@tochka.com"
        }
    ],
    "keywords": [
        "jsonrpc",
        "openrpc",
        "laravel"
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "illuminate/support": "^6.0|^7.0|^8.0",
        "illuminate/container": "^6.0|^7.0|^8.0",
        "illuminate/pipeline": "^6.0|^7.0|^8.0",
        "doctrine/annotations": "^1.8",
        "doctrine/cache": "^1.8",
        "tochka-developers/jsonrpc": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tochka\\OpenRpc\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tochka\\OpenRpc\\OpenRpcServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
