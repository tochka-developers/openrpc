{
    "name": "tochka-developers/openrpc",
    "description": "OpenRpc extension for tochka-developers/jsonrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Babushkin",
            "email": "darkdarin@tochka.com"
        }
    ],
    "keywords": [
        "jsonrpc",
        "openrpc",
        "laravel"
    ],
    "require": {
        "php": "8.2.*|8.3.*|8.4.*",
        "ext-json": "*",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/container": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/pipeline": "^9.0|^10.0|^11.0|^12.0",
        "spiral/attributes": "^2.8|^3.0",
        "tochka-developers/jsonrpc": "^4.0",
        "tochka-developers/jsonrpc-annotations": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5|^10.0|^11.0",
        "mockery/mockery": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Tochka\\OpenRpc\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tochka\\OpenRpc\\OpenRpcServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
