{
    "name": "tochka-developers/openrpc",
    "description": "OpenRpc extension for tochka-developers/jsonrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Babushkin",
            "email": "darkdarin@tochka.com"
        }
    ],
    "keywords": [
        "jsonrpc",
        "openrpc",
        "laravel"
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/console": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/container": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/pipeline": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "spiral/attributes": "^2.8",
        "tochka-developers/jsonrpc": "^4.0|^5.0",
        "tochka-developers/jsonrpc-annotations": "^1.1"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5|^10.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tochka\\OpenRpc\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tochka\\OpenRpc\\OpenRpcServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
